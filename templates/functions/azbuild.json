{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "functions": [
        {
            "namespace": "azBuild",
            "members": {
                "setResourceGroupProperty": {
                    "parameters": [
                        {
                            "name": "inputObject",
                            "type": "object"
                        },
                        {
                            "name": "propertyName",
                            "type": "string"
                        }
                    ],
                    "output": {
                        "type": "string",
                        "value": "[if(contains(parameters('inputObject'), parameters('propertyName')), parameters('inputObject').parameters, resourceGroup().id)]"
                    }
                }
            }
        }
    ],
    "resources": []
}